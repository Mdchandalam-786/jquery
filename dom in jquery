<button class="btn">parent</button>
<button class="btn2">parents</button>
<button class="btn3">parents Until</button>
<button class="btn4">find</button>
<button class="btn5">siblings</button>
<button class="btn6">next</button>
<button class="btn7">prev</button>
<button class="btn8">nextAll</button>
<button class="btn9">prevAll</button>
<button class="btn10">nextUntil</button>
<button class="btn11">prevUntil</button>

<script type="text/javascript">
	$(document).ready(function(){
		$(".btn").click(function(){
			$(".child_two").parent().css("background-color","blue");
		});
		$(".btn2").click(function(){
			$(".child_two").parents().css("background-color","blue");
		});
		$(".btn3").click(function(){
			$(".child_two").parentsUntil().css("background-color","cyan")
		});
		$(".btn4").click(function(){
			$(".great-grand-parent").find(".child_four").css("background-color","blue")
		});
		$(".btn5").click(function(){
			$(".child_three").siblings().css("background-color","pink");
		});
		$(".btn6").click(function(){
			$(".child_five").next().css("background-color","yellow");
		});
		$(".btn7").click(function(){
			$(".child_six").prev().css("background-color","orange");
		});
		$(".btn8").click(function(){
			$(".child_five").nextAll().css("background-color","brown");
		});
		$(".btn9").click(function(){
			$(".child_six").prevAll().css("background-color","red");
		});
		$(".btn10").click(function(){
			$(".child_two").nextUntil().css("background-color","brown");
		});
		$(".btn11").click(function(){
			$(".child_six").prevUntil().css("background-color","black");
		});
	});
</script>
